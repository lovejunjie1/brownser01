global proc gtGrowTop(){

string $name=`textFieldButtonGrp -q -tx gtTextField`;
$name=($name+"_");

string $pathObj=`textFieldButtonGrp -q -tx gtTextFielda`;
string $pathProfile=`textFieldButtonGrp -q -tx gtTextFieldb`;

