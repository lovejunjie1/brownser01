       $expLJF+="_dustShape.age/";
       $expLJF+=($name);
       $expLJF+="_dustShape.lifespan);\n";

       $expLJF+="setAttr ";
       $expLJF+=($name);
       $expLJF+="_particlePosition.parameter ($age*"+$max+");\n";

       $expLJF+="float $pos[]=`getAttr ";
       $expLJF+=($name);
       $expLJF+="_particlePosition.position`;\n";

       $expLJF+="float $rad=$rootRadius+pow(";
