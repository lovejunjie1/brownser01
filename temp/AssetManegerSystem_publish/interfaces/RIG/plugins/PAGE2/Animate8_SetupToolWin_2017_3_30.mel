         int $stat=`getAttr ($sel[$i]+".v")`;
        if($stat==0){
           setAttr ($sel[$i]+".v") 1;
        }else{
          setAttr ($sel[$i]+".v") 0;  
        }
        if($lock==1){
          setAttr -lock 1 ($sel[$i]+".v");  
            }
        }
    }
}
