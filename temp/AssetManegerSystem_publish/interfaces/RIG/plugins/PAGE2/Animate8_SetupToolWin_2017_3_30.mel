       $expLJF+="Con.rootRadius`;\n";

       $expLJF+="float $topGrow=`getAttr ";
       $expLJF+=($name);
       $expLJF+="Con.topGrow`;\n";

       $expLJF+="float $sprialNum=`getAttr ";
