global proc gtSModifyAttribute(){
string $sel[]=`ls -sl`;
string $name=`textField -q -text gtSTextF`;
float $value=`floatSliderGrp -q -v gtSfloatF`;

string $name2=`textField -q -text gtSTextF2`;
